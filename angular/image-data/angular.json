{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "projects": {
      "protoangularlib": {
        "projectType": "library",
        "root": "./src",
        "sourceRoot": "./src",
        "prefix": "lib",
        "architect": {
          "build": {
            "builder": "@angular-devkit/build-angular:ng-packagr",
            "options": {
              "tsConfig": "./src/tsconfig.json",
              "project": "./src"
            },
            "configurations": {
              "production": {
                "tsConfig": "./src/tsconfig.json"
              }
            }
          }
        }
      }
    },
    "defaultProject": "protoangularlib"
  }